{
  "name": "knowledge-base",
  "version": "1.0.0",
  "description": "RAGFlow knowledge-base proxy with Langfuse logging",
  "private": true,
  "type": "module",
  "workspaces": [
    "be",
    "fe"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev:be\" \"npm run dev:fe\"",
    "dev:be": "npm run dev --workspace=be",
    "dev:fe": "npm run dev --workspace=fe",
    "dev:https": "concurrently \"npm run dev:be\" \"npm run dev:fe\"",
    "generate:cert": "node scripts/generate-cert.js",
    "build": "npm run build --workspaces",
    "build:prod": "npm run build:prod --workspaces",
    "build:be": "npm run build --workspace=be",
    "build:fe": "npm run build --workspace=fe",
    "lint": "npm run lint --workspaces",
    "test": "npm run test --workspaces --if-present"
  },
  "devDependencies": {
    "@testing-library/react": "^16.3.1",
    "@types/jest": "^30.0.0",
    "@types/jszip": "^3.4.0",
    "@types/mocha": "^10.0.10",
    "@types/nprogress": "^0.2.3",
    "concurrently": "^9.1.0",
    "dotenv": "^16.4.5",
    "rimraf": "^5.0.0",
    "typescript": "^5.9.3",
    "vite-plugin-top-level-await": "^1.6.0",
    "vite-plugin-wasm": "^3.5.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "bee-queue": "^2.0.0",
    "nprogress": "^0.2.0"
  }
}